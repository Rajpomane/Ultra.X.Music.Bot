{
    "name": "ULTRA X MUSIC BOT",
    "description": "Music allow you to stream music trought the telegram voice chat feature.",
    "keywords": ["OFFICIALHACKERERA", "-Vc-Player", "music", "voice chat", "telegram", "ULTRA", "ULTRA X Music", "Private Music"],
    "repository": "https://github.com/OFFICIALHACKERERA/ULTRA-X-MUSIC-BOT",
    "logo": "https://telegra.ph/file/ac2cbc52c602ed4dbc549.jpg",
  "stack": "container",
    "env": {
        "DEEPAK_STRING": {
            "description": "fill with the pyrogram session string from account",
            "required": true
        },
        "API_ID": {
            "description": "your Api ID from my.telegram.org/apps",
            "required": true,
            "value": ""
        },
        "API_HASH": {
            "description": "your Api Hash from my.telegram.org/apps",
            "required": false,
            "value": ""
        },
        "LOGGER_ID": {
          "description": "Create a private channel. And get it's id from @missrose_bot and paste here. Value should start with -100",
          "value": "",
          "required": true
        },
        "ASSISTANT_NAME": {
            "description": "fill with your telegram id as the owner of the bot",
            "required": true,
            "value": "ULTRA_X_ASSISTANT"
        },
        "GROUP_SUPPORT": {
            "description": "Ur Support Group Username",
            "required": false,
            "value": "HEPPYLIFI"
        },
        "UPDATES_CHANNEL": {
            "description": "Your Updates Channel",
            "required": false,
            "value": "Broken_Heart_72"
        },
        "SUDO_USERS": {
            "description": "fill with the user id who can access all function in your bot (separate with space).",
            "required": true,
            "value": "2091769666"
        },
        "DURATION_LIMIT": {
            "description": "filled, don't change this !",
            "required": true,
            "value": "50000"
        },
        "BOT_TOKEN": {
            "description": "fill with your bot token get from @BotFather on telegram",
            "required": true,
            "value": ""
        }

    }
  }
